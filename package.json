{
  "name": "myquiz-workspace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:frontend": "cd myquiz-main && npm install",
    "install:backend": "cd myquiz-main/backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "dev": "cd myquiz-main && npm run dev",
    "dev:frontend": "cd myquiz-main && npm run dev",
    "dev:backend": "cd myquiz-main/backend && npm run dev",
    "start:backend": "cd myquiz-main/backend && npm start"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.3.1"
  }
}
