{
  "buildCommand": "cd myquiz-main && npm install && npm run build",
  "outputDirectory": "myquiz-main/dist",
  "installCommand": "npm install --prefix myquiz-main",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Vercel-SPA",
          "value": "true"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
