{
  "buildCommand": "npm run build --prefix myquiz-main",
  "outputDirectory": "myquiz-main/dist",
  "installCommand": "npm install --prefix myquiz-main",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
